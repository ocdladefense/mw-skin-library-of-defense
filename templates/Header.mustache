
<div class="ocdla-header-container">
	<header class="ocdla-header">
   		<a href="{{link-mainpage}}">
			{{#data-logos}}
				{{#icon}}<img src="{{.}}">{{/icon}}
				{{#wordmark}}<img src="{{src}}" width="{{width}}" height="{{height}}">{{/wordmark}}
				{{^wordmark}}<span id="ocdla-subtitle">{{msg-tagline}}</span>{{/wordmark}}
			{{/data-logos}}
			</a>
	</header>
	<h2>{{#entrance}}<span>{{entrance.entrance-title}} is set in SkinExample</span>{{/entrance}}</h2>
			<!--site navigation-->
		<div id="site-navigation">
			<h1>{{data-portlets-sidebar.foo}} is on SkinTemplate line 703</h1>
			
			{{#data-portlets-sidebar.data-portlets-first}}{{>Portlet}}{{/data-portlets-sidebar.data-portlets-first}}
			<div class="mw-default-hidden">{{#data-portlets-sidebar.array-portlets-rest}}{{>Portlet}}{{/data-portlets-sidebar.array-portlets-rest}}</div>
			{{#data-portlets.data-languages}}{{>Portlet}}{{/data-portlets.data-languages}}
		</div>
</div>